<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>WebPulse Safe Search</ShortName>
<Description>WebPulse Safe Search</Description>
<InputEncoding>UTF-8</InputEncoding>
<Image height="16" width="16" type="image/x-icon">http://www.wpsafesearch.com/favicon.ico</Image>

<Url type="text/html" template="http://www.wpsafesearch.com/search.jsp">
  <Param name="r" value="o"/>
  <Param name="q" value="{searchTerms}"/> 
</Url>
</SearchPlugin>
